{
	"name": "omi",
	"displayName": "omi",
	"description": "Omi for Visual Studio Code",
	"license": "Omi",
	"version": "1.0.3",
	"publisher": "ZainChen",
	"author": {
		"name": "ZainChen"
	},
	"icon": "assets/omi-logo2019-128.png",
	"homepage": "https://github.com/ZainChen/vscode-omi/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/ZainChen/vscode-omi"
	},
	"bugs": {
		"url": "https://github.com/ZainChen/vscode-omi/issues"
	},
	"keywords": [
		"zain",
		"ZainChen",
		"zy",
		"银",
		"志银",
		"陈志银",
		"陈志振",
		"omi",
		"json",
		"html",
		"css",
		"javascript",
		"js",
		"nodejs",
		"github",
		"gitlab",
		"git",
		"c",
		"C++",
		"web",
		"js",
		"npm",
		"python",
		"java",
		"Ruby",
		"webpack",
		"php",
		"mysql",
		"sql",
		"apache",
		"nginx",
		"iis",
		"wordpress",
		"hexo",
		"tencent",
		"linux",
		"window",
		"mac",
		"vs",
		"google",
		"markdown",
		"xml",
		"run",
		"commands",
		"view",
		"diff",
		"tree",
		"compare",
		"vscode"
	],
	"categories": [
		"Snippets",
		"Other"
	],
	"engines": {
		"vscode": "^1.31.0"
	},
	"activationEvents": [
		"onView:omi.view.ecosystem",
		"onView:fileExplorer",
		"onCommand:omi.cmd.ecoRefresh",
		"onCommand:omi.cmd.ecoRefreshNode",
		"onCommand:omi.cmd.help",
		"onCommand:omi.cmd.webview",
		"onLanguage:c",
		"onLanguage:cpp",
		"onLanguage:css",
		"onLanguage:html",
		"onLanguage:javascript",
		"onLanguage:javascriptreact",
		"onLanguage:markdown",
		"onLanguage:php",
		"onLanguage:python",
		"onLanguage:json",
		"onLanguage:jsonc",
		"onLanguage:objective-c",
		"onLanguage:xml",
		"onLanguage:sql",
		"onLanguage:java",
		"onLanguage:swift",
		"onLanguage:tex",
		"onLanguage:go",
		"onLanguage:csharp"
	],
	"main": "./src/index.js",
	"contributes": {
		"commands": [
			{
				"command": "omi.cmd.ecoRefresh",
				"title": "omi refresh ecosystem",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "omi.cmd.ecoRefreshNode",
				"title": "omi refresh node ecosystem",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "omi.cmd.help",
				"title": "omi help",
				"icon": {
					"light": "./assets/omi-set.svg",
					"dark": "./assets/omi-set.svg"
				}
			},
			{
				"command": "omi.cmd.webview",
				"title": "omi wv",
				"icon": {
					"light": "./assets/add1.svg",
					"dark": "./assets/add1.svg"
				}
			},
			{
				"command": "fileExplorer.openFile",
				"title": "Open File"
			},
			{
				"command": "extension.demo.showWelcome",
				"title": "Displays a custom welcome page"
			}
		],
		"languages": [
			{
				"id": "html",
				"extensions": [
					".html"
				],
				"aliases": [
					"HTML",
					"html"
				]
			},
			{
				"id": "json",
				"extensions": [
					".json"
				],
				"aliases": [
					"Json"
				]
			},
			{
				"id": "javascript",
				"extensions": [
					".js"
				],
				"aliases": [
					"JavaScript",
					"js",
					"JS",
					"Js"
				]
			},
			{
				"id": "tex",
				"extensions": [
					".txt"
				],
				"aliases": [
					"text"
				]
			}
		],
		"snippets": [
			{
				"path": "./src/snippets/omi.code-snippets"
			},
			{
				"language": "html",
				"path": "./src/snippets/omiu.json"
			},
			{
				"language": "javascript",
				"path": "./src/snippets/omiu.json"
			},
			{
				"language": "javascript",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "typescript",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "json",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "html",
				"path": "./src/snippets/omi.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "omi-views",
					"title": "omi",
					"icon": "./assets/omi-logo-50-40-28.svg"
				}
			]
		},
		"views": {
			"omi-views": [
				{
					"id": "omi.view.ecosystem",
					"name": "ecosystem"
				}
			]
		},
		"menus": {
			"editor/context": [
				{
					"command": "omi.cmd.help",
					"when": "editorFocus",
					"group": "navigation"
				},
				{
					"when": "resourceLangId == markdown",
					"command": "markdown.showPreview",
					"alt": "markdown.showPreviewToSide",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "omi.cmd.help",
					"group": "navigation"
				},
				{
					"command": "markdown.showPreview",
					"when": "resourceLangId == markdown",
					"alt": "markdown.showPreviewToSide",
					"group": "navigation"
				}
			],
			"view/title": [
				{
					"command": "omi.cmd.ecoRefresh",
					"when": "view == omi.view.ecosystem",
					"group": "navigation@2"
				},
				{
					"command": "omi.cmd.help",
					"when": "view == omi.view.ecosystem",
					"group": "navigation@1"
				},
				{
					"command": "omi.cmd.webview",
					"when": "view == omi.view.ecosystem"
				}
			],
			"view/item/context": [
				{
					"command": "omi.cmd.ecoRefreshNode",
					"when": "view == omi.view.ecosystem",
					"group": "inline"
				}
			]
		},
		"keybindings": [
			{
				"command": "omi.cmd.help",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Omi",
			"properties": {
				"omi.author.dntzhang": {
					"type": "string",
					"default": "Welcome to omi.",
					"description": "Omi for Visual Studio Code"
				},
				"omi.start.welcome": {
					"type": "boolean",
					"default": true,
					"description": "Whether to display a welcome prompt every time you start!"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2",
		"cheerio": "^1.0.0-rc.2",
		"request": "^2.88.0",
		"request-promise": "^4.2.4"
	}
}
