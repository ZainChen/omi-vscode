		"onLanguage:go",
		"onLanguage:csharp"
	],
	"main": "./src/index.js",
	"contributes": {
		"commands": [
			{
				"command": "omi.cmd.ecoHello",
				"title": "Github switches to omi",
				"icon": {
					"light": "./assets/light/omi.svg",
					"dark": "./assets/dark/omi.svg"
				}
			},
			{
				"command": "omi.cmd.ecoGithubSwitch",
				"title": "omi Github Switch",
				"icon": {
					"light": "./assets/light/github.svg",
					"dark": "./assets/dark/github.svg"
				}
			},
			{
				"command": "omi.cmd.ecoRefresh",
				"title": "omi refresh ecosystem",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "omi.cmd.ecoRefreshNode",
				"title": "omi refresh node ecosystem",
				"icon": {
					"light": "./assets/light/refresh.svg",
					"dark": "./assets/dark/refresh.svg"
				}
			},
			{
				"command": "omi.cmd.ecoOpenGithub",
				"title": "open github",
				"icon": {
					"light": "./assets/light/github-web.svg",
					"dark": "./assets/dark/github-web.svg"
				}
			},
			{
				"command": "omi.cmd.ecoClearCache",
				"title": "clear cache",
				"icon": {
					"light": "./assets/add1.svg",
					"dark": "./assets/add1.svg"
				}
			},
			{
				"command": "extension.demo.showWelcome",
				"title": "Displays a custom welcome page"
			}
		],
		"languages": [
			{
				"id": "html",
				"extensions": [
					".html"
				],
				"aliases": [
					"HTML",
					"html"
				]
			},
			{
				"id": "json",
				"extensions": [
					".json"
				],
				"aliases": [
					"Json"
				]
			},
			{
				"id": "javascript",
				"extensions": [
					".js"
				],
				"aliases": [
					"JavaScript",
					"js",
					"JS",
					"Js"
				]
			},
			{
				"id": "tex",
				"extensions": [
					".txt"
				],
				"aliases": [
					"text"
				]
			}
		],
		"snippets": [
			{
				"path": "./src/snippets/omi.code-snippets"
			},
			{
				"language": "html",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "javascript",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "javascriptreact",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "typescript",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "typescriptreact",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "json",
				"path": "./src/snippets/omi.json"
			},
			{
				"language": "html",
				"path": "./src/snippets/omiu.json"
			},
			{
				"language": "javascript",
				"path": "./src/snippets/omiu.json"
			},
			{
				"language": "javascriptreact",
				"path": "./src/snippets/omiu.json"
			},
			{
				"language": "typescript",
				"path": "./src/snippets/omiu.json"
			},
			{
				"language": "typescriptreact",
				"path": "./src/snippets/omiu.json"
			},
			{
				"language": "json",
				"path": "./src/snippets/omiu.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "omi-views",
					"title": "omi",
					"icon": "./assets/omi-logo-50-40-28.svg"
				}
			]
		},
		"views": {
			"omi-views": [
				{
					"id": "omi.view.ecosystem",
					"name": "ecosystem"
				}
			]
		},
		"menus": {
			"editor/context": [
				{
					"command": "omi.cmd.ecoHello",
					"when": "editorFocus",
					"group": "navigation"
				},
				{
					"when": "resourceLangId == markdown",
					"command": "markdown.showPreview",
					"alt": "markdown.showPreviewToSide",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "omi.cmd.ecoHello",
					"group": "navigation"
				},
				{
					"command": "markdown.showPreview",
					"when": "resourceLangId == markdown",
					"alt": "markdown.showPreviewToSide",
					"group": "navigation"
				}
			],
			"view/title": [
				{
					"command": "omi.cmd.ecoGithubSwitch",
					"when": "view == omi.view.ecosystem",
					"group": "navigation@2"
				},
				{
					"command": "omi.cmd.ecoRefresh",
					"when": "view == omi.view.ecosystem",
					"group": "navigation@3"
				},
				{
					"command": "omi.cmd.ecoHello",
					"when": "view == omi.view.ecosystem",
					"group": "navigation@1"
				},
				{
					"command": "omi.cmd.ecoClearCache",
					"when": "view == omi.view.ecosystem"
				}
			],
			"view/item/context": [
				{
					"command": "omi.cmd.ecoOpenGithub",
					"when": "view == omi.view.ecosystem",
					"group": "inline@1"
				},
				{
					"command": "omi.cmd.ecoRefreshNode",
					"when": "view == omi.view.ecosystem",
					"group": "inline@2"
				}
			]
		},
		"keybindings": [
			{
				"command": "omi.cmd.ecoHello",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Omi",
			"properties": {
				"omi.author.dntzhang": {
					"type": "string",
					"default": "Welcome to omi.",
					"description": "Omi for Visual Studio Code"
				},
				"omi.start.welcome": {
					"type": "boolean",
					"default": true,
					"description": "Whether to display a welcome prompt every time you start!"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"engines": {
		"vscode": "^1.31.0"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2",
		"cheerio": "^1.0.0-rc.2",
		"request": "^2.88.0",
		"request-promise": "^4.2.4"
	}
}
