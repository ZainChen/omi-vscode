{
	"name": "omi",
	"displayName": "omi",
	"description": "Visual Studio Code extension for Omi",
	"license": "Omi",
	"version": "2.2.0",
	"publisher": "ZainChen",
	"author": {
		"name": "ZainChen"
	},
	"icon": "assets/omi-logo2019-128.png",
	"homepage": "https://github.com/ZainChen/omi-vscode/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/ZainChen/omi-vscode"
	},
	"bugs": {
		"url": "https://github.com/ZainChen/omi-vscode/issues"
	},
	"keywords": [
		"AlloyTeam",
		"张磊",
		"当耐特",
		"dntzhang",
