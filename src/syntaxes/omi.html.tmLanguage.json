{
	"scopeName": "omi.source.html.json",
	"injectionSelector": "L:meta.tag.custom.start.html",
	"patterns": [
	  {
		"include": "#omi-html-attributes-json"
	  }
	],
	"repository": {
	  "omi-html-attributes-json": {
			"contentName": "meta.embedded.omi.html.attributes.json",
			"begin": "(?x)(\\b(?:\\w+\\.)*(?:=)\\s*)(\")",
			"end": "(\")",
			"beginCaptures": {
				"2": {
					"name": "string.js"
				},
				"3": {
					"name": "punctuation.definition.string.template.begin.js"
				}
			},
			"endCaptures": {
				"0": {
					"name": "string.js"
				},
				"1": {
					"name": "punctuation.definition.string.template.end.js"
				}
			},
			"patterns": [
				{"include": "punctuation.separator.comma.js"},
				{"include": "meta.objectliteral.js"},
				{"include": "meta.array.literal.js"},
				{"include": "meta.embedded.expression.js"},
				{"include": "meta.tag.attributes.js"},
				{"include": "meta.tag.js"},
				{"include": "meta.jsx.children.js"},
				{"include": "meta.tag.without-attributes.js"},
				{"include": "source.js"}
			]
		}
	}
}