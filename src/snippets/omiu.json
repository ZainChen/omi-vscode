{
    "o-button": {
        "prefix": "o-button",
        "body": "<o-button>${1:button}</o-button>",
        "description": "[omiu] Click or touch it to trigger an operation. The encapsulated logic is triggered in response to user clicks."
    },
    "<o-icon />": {
        "prefix": "o-icon",
        "body": "<o-icon ${1} />",
        "description": "[omiu] Icon using svg."
    },
    "<o-icon paths />": {
        "prefix": "o-icon",
        "body": [
            "<o-icon paths={[{",
            "\tcolor: '${1:string}',",
            "\tpath: '${2:string}'",
            "}, {",
            "\tcolor: '${3:string}',",
            "\tpath: '${4:string}'",
            "}]} />"
        ],
        "description": "[omiu] Icon using svg."
    },
    "<o-icon paths /></o-icon>": {
        "prefix": "o-icon",
        "body": [
            "<o-icon paths={[{",
            "\tcolor: '${1:string}',",
            "\tpath: '${2:string}'",
            "}, {",
            "\tcolor: '${3:string}',",
            "\tpath: '${4:string}'",
            "}]}>${5:string}</o-icon>"
        ],
        "description": "[omiu] Icon using svg."
    },
    "<o-icon></o-icon>": {
        "prefix": "o-icon",
        "body": "<o-icon ${1}>${2}</o-icon>",
        "description": "[omiu] Icon using svg."
    },
    "o-row": {
        "prefix": "o-row",
        "body": [
            "<o-row>",
            "\t<col span={${1}}>${2}</col>",
            "</o-row>"
        ],
        "description": "[omiu] Row of the 24 grid system."
    },
    "o-equal-space": {
        "prefix": "o-equal-space",
        "body": "<o-equal-space>${1}</o-equal-space>",
        "description": "[omiu] Horizontal average allocation space layout."
    },
    "o-pagination": {
        "prefix": "o-pagination",
        "body": [
            "<o-pagination",
            "\ttotal={${1:number}}",
            "\tcurrentPage={${2:number}}",
            "\tpageSize={${3:number}}",
            "\tonChange={${4:function}}",
            "/>"
        ],
        "description": "[omiu] Paging component."
    },
    "o-timeline": {
        "prefix": "o-timeline",
        "body": [
            "<o-timeline data={[",
            "\t{${1}}",
            "]} />"
        ],
        "description": "[omiu] Time or step schedule."
    },
    "o-nav": {
        "prefix": "o-nav",
        "body": [
            "<o-nav>",
            "\t<item>${1:string}</item>",
            "</o-nav>"
        ],
        "description": "[omiu] Navigation."
    },
    "o-tab": {
        "prefix": "o-tab",
        "body": [
            "<o-tab>",
            "\t<item>${1:string}</item>",
            "</o-tab>"
        ],
        "description": "[omiu] Omiu is the official UI component library of Omi, based on Omio(IE8+) and compatible with Omi, using the design style of Wechat 7.0."
    },
    "o-dropdown-menu": {
        "prefix": "o-dropdown-menu",
        "body": [
            "<o-dropdown-menu>",
            "\t<item icon={{ path: '${1:path string}', color: '${2:any color string}' }} text='${3:any text string}'></item>",
            "</o-dropdown-menu>"
        ],
        "description": "[omiu] DropdownMenu."
    },
    "o-action-sheet": {
        "prefix": "o-action-sheet",
        "body": [
            "<o-action-sheet",
            "\tmenus={${1:json array}}",
            "\tactions={${2:json array}}",
            "\tshow={${3:bool}}",
            "\ttype=\"${4:string}\"",
            "\tonClose={e => { ${5} }}",
            "/>"
        ],
        "description": "[omiu] Selection component in pop-up mode."
    },
    "o-toast": {
        "prefix": "o-toast",
        "body": "<o-toast type='${1:string}' show={${2:bool}}>${3:string}</o-toast>",
        "description": "[omiu] Simple reminder notification."
    },
    "o-popup": {
        "prefix": "o-popup",
        "body": [
            "<o-popup",
            "\tonClose={${1:function}}",
            "\tonConfirm={${2:function}}",
            "\tshow={${3:bool}}",
            "\ttitle='${4:string}'",
            "\tcancelText='${5:string}'",
            "\tconfirmText='${6:string}'>",
            "\t<div>${7:string}</div>",
            "</o-popup>"
        ],
        "description": "[omiu] Popup content layer."
    },
    "o-dialog": {
        "prefix": "o-dialog",
        "body": [
            "<o-dialog",
            "\tonClose={${1:function}}",
            "\tonConfirm={${2:function}}",
            "\tshow={${3:bool}}",
            "\ttype='${4:string}'",
            "\ttitle='${5:string}'",
            "\tmsg='${6:string}'",
            "\tcancelText='${7:string}'",
            "\tconfirmText='${8:string}'",
            "/>"
        ],
        "description": "[omiu] Alert or confirm dialog."
    },
    "o-popover": {
        "prefix": "o-popover",
        "body": [
            "<o-popover",
            "\tshow={${1:bool}}",
            "\ttarget={${2:dom element}}",
            "\tdirection={${3:string}}",
            "\tonClose='${4:function}'>",
            "\t<div>${5:string}</div>",
            "</o-popover>"
        ],
        "description": "[omiu] Popover."
    },
    "o-input": {
        "prefix": "o-input",
        "body": "<o-input  placeholder=\"${1:string}\" />",
        "description": "[omiu] User's input box, enter some content for submission to the server."
    },
    "o-input-label": {
        "prefix": "o-input-label",
        "body": "<o-input-label label='${1:string}' />",
        "description": "[omiu] User's input box, enter some content for submission to the server."
    },
    "o-input-button": {
        "prefix": "o-input-button",
        "body": "<o-input-button buttonText='${1:string}' onClick={${2:function}} />",
        "description": "[omiu] User's input box, enter some content for submission to the server."
    },
    "o-input-number": {
        "prefix": "o-input-number",
        "body": [
            "<o-input-number",
            "\tonChange={${1:function}}",
            "\tmin={${2:number}}",
            "\tmax={${3:number}}",
            "\tstep={${4:number}}",
            "\tvalue={${5:number}}",
            "\tlabel=\"${6:string}\"",
            "/>"
        ],
        "description": "[omiu] Digital input box."
    },
    "<o-checkbox></o-checkbox>": {
        "prefix": "o-checkbox",
        "body": "<o-checkbox checked='${1:bool}' label='${2:string}'></o-checkbox>",
        "description": "[omiu] Checkbox."
    },
    "<o-checkbox />": {
        "prefix": "o-checkbox",
        "body": "<o-checkbox checked='${1:bool}' label='${2:string}' />",
        "description": "[omiu] Checkbox."
    },
    "<o-radio></o-radio>": {
        "prefix": "o-radio",
        "body": "<o-radio name='${1:string}' checked='${2:bool}' label='${3:string}'></o-radio>",
        "description": "[omiu] Radio."
    },
    "<o-radio />": {
        "prefix": "o-radio",
        "body": "<o-radio name='${1:string}' checked='${2:bool}' label='${3:string}' />",
        "description": "[omiu] Radio."
    },
    "o-dropdown": {
        "prefix": "o-dropdown",
        "body": [
            "<o-dropdown",
            "\tselectedIndex={${1:number}}",
            "\tshow={${2:this.showSelect}}",
            "\tonToggle={${3:this.onToggle}}",
            "\tonSelect={${4:this.onSelect}}>",
            "\t<item value={${5}}>${6:Item1}</item>",
            "</o-dropdown>"
        ],
        "description": "[omiu] Dropdown."
    },
    "<o-switch></o-switch>": {
        "prefix": "o-switch",
        "body": "<o-switch onChange={${1:function}} checked={${2:bool}}></o-switch>",
        "description": "[omiu] Switch."
    },
    "<o-switch />": {
        "prefix": "o-switch",
        "body": "<o-switch onChange={${1:function}} checked={${2:bool}} />",
        "description": "[omiu] Switch."
    },
    "o-date-picker": {
        "prefix": "o-date-picker",
        "body": [
            "<o-date-picker",
            "\tselectedDate={${1:string}}",
            "\tonSelect={${2:function}}",
            "\tshow={${3:bool}}",
            "\tonToggle={${4:function}}>",
            "</o-date-picker>"
        ],
        "description": "[omiu] Date picker."
    },
    "o-rate": {
        "prefix": "o-rate",
        "body": "<o-rate total={${1:number}} value={${2:number}} color='${3:string}' onSelect={${4:function}} />",
        "description": "[omiu] Rate."
    },
    "o-select-list": {
        "prefix": "o-select-list",
        "body": [
            "<o-select-list",
            "\tonSelect={${1:function}}",
            "\tlist={${2:json array}}",
            "\tselectedIndex={${3:number}}",
            "/>"
        ],
        "description": "[omiu] SelectList."
    },
    "o-table": {
        "prefix": "o-table",
        "body": [
            "<o-table",
            "\tref={e => { ${1} }}",
            "\tdataSource={${2:json array}}",
            "\tcolumns={${3}}>",
            "</o-table>"
        ],
        "description": "[omiu] Data is presented and processed in the form of tables."
    },
    "o-input-table": {
        "prefix": "o-input-table",
        "body": [
            "<o-input-table ",
            "\tdataSource={${2:json array}}",
            "\tcolumns={${3:json array}}>",
            "</o-input-table>"
        ],
        "description": "[omiu] Editable table."
    },
    "o-badge": {
        "prefix": "o-badge",
        "body": [
            "<o-badge content=${1:string}>",
            "\t<o-icon ${2} />",
            "</o-badge>"
        ],
        "description": "[omiu] The number of circular logo in the upper right corner of the icon."
    },
    "o-progress": {
        "prefix": "o-progress",
        "body": "<o-progress bgColor='${1:string}' innerColor='${2:string}' value={${3:number}} />",
        "description": "[omiu] Show the progress of a task."
    },
    "o-path-progress": {
        "prefix": "o-path-progress",
        "body": [
            "<o-path-progress",
            "\ttype='${1:string}'",
            "\tprogress={${2:number}}",
            "\ttext={'${3:string}'}",
            "\toptions={${4:json}}",
            "\tinitialAnimate={${5:bool}}",
            "\tcontainerStyle={{",
            "\t\twidth: '${6}',",
            "\t\theight: '${7}'",
            "\t}}",
            "\tcontainerClassName={'${8:string}'}",
            "></o-path-progress>"
        ],
        "description": "[omiu] Progress by SVG."
    },
    "o-step": {
        "prefix": "o-step",
        "body": "<o-step items={${1:json array}} />",
        "description": "[omiu] Step of workflow or other."
    }
}